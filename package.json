{
  "name": "caf_tracker",
  "version": "1.0.0",
  "private": true,
  "description": "Server of MoKee CAF tracker",
  "main": "build/server.prod.js",
  "scripts": {
    "dev": "./node_modules/.bin/babel-node src/server.dev.js",
    "dev:fetch": "./node_modules/.bin/babel-node src/fetch.js",
    "start": "NODE_ENV=production node build/server.prod.js",
    "start:fetch": "NODE_ENV=production node build/fetch.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "./node_modules/.bin/webpack --config webpack.prod.js",
    "build:server": "./node_modules/.bin/babel src -d build",
    "clean": "rm -rf build && rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoKee/caf_tracker.git"
  },
  "author": "XiNGRZ <chenxingyu92@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/MoKee/caf_tracker/issues"
  },
  "homepage": "https://github.com/MoKee/caf_tracker#readme",
  "dependencies": {
    "ajv": "^5.2.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.1",
    "express": "^4.15.4",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "mysql2": "^1.4.2",
    "pug": "^2.0.0-rc.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "sequelize": "^4.8.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "flying-assets-webpack-plugin": "0.1.0-rc6",
    "node-sass": "^4.5.3",
